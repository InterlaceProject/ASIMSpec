CoreASIM scheduler

use Standard

init Start

controlled counter:INTEGER

scheduling whileRunning

policy whileRunning = schedule client

rule Start = {
	counter := 0
	program(self) := Program
}

rule Program = {
	//print "Scheduling ..." + counter
	counter := counter + 1
	/*
	forall a in Agents do
		print a
	endforall
	*/
	
}
