CoreASIM client

use Standard

init Start
controlled c: INTEGER

scheduling whileRunning

policy whileRunning = skip

rule Start = {
	c := 0
	program(self) := Program
}

rule CreditReviewReq(req_params) = {
	print "request params: " + req_params
}

rule Program = {
	c := c + 1
	if c <= 10 then par
		send "from, to, id: " + c  to "agent" with subject "CreditReviewRequest"
		print "sending" + "from, to, id: " + c  to "agent"
	endpar
}
